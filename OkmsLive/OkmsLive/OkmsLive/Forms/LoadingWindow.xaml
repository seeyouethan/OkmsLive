<Window x:Class="OkmsLive.Forms.LoadingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OkmsLive.Forms"
        xmlns:userControlers="clr-namespace:OkmsLive.UserControllers"
        mc:Ignorable="d"
        Title="LoadingWindow" Height="120" Width="120" WindowStyle="None" WindowStartupLocation="CenterScreen" BorderThickness="0">
    <Grid>
        <userControlers:LoadingWait x:Name="UserLoading" Visibility="Visible"/>
    </Grid>
</Window>
